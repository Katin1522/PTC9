<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/MainActivityHabitacionesOcupadas"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivityHabitacionesOcupadas">

    <TextView
        android:id="@+id/DatosGenerales"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Habitaciones ocupadas"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="79dp"
        tools:layout_editor_absoluteY="54dp" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="97dp"
        android:layout_height="78dp"
        tools:layout_editor_absoluteX="277dp"
        tools:layout_editor_absoluteY="54dp"
        tools:srcCompat="@tools:sample/avatars" />

    <Button
        android:id="@+id/btnActualizarHab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Actualizar"
        tools:layout_editor_absoluteX="95dp"
        tools:layout_editor_absoluteY="630dp" />

    <Button
        android:id="@+id/btnEliminarHab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Eliminar"
        tools:layout_editor_absoluteX="270dp"
        tools:layout_editor_absoluteY="630dp" />

    <androidx.recyclerview.widget.RecyclerView
        android:layout_width="409dp"
        android:layout_height="467dp"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="147dp" />

</androidx.constraintlayout.widget.ConstraintLayout>