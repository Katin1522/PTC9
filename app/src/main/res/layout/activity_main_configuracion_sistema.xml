<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/txtHistorialExp"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity_ConfiguracionSistema">

    <TextView
        android:id="@+id/textView8"
        android:layout_width="222dp"
        android:layout_height="29dp"
        android:text="Configuracion del Sistema "
        android:textSize="16sp"
        tools:layout_editor_absoluteX="125dp"
        tools:layout_editor_absoluteY="45dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="104dp"
        android:layout_height="92dp"
        tools:layout_editor_absoluteX="273dp"
        tools:layout_editor_absoluteY="96dp"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nombre"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="50dp"
        tools:layout_editor_absoluteY="159dp" />

    <EditText
        android:id="@+id/txtnombreNuevo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Agregue el nombre"
        android:inputType="text"
        tools:layout_editor_absoluteX="43dp"
        tools:layout_editor_absoluteY="191dp" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Usuario"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="43dp"
        tools:layout_editor_absoluteY="279dp" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Contraseña"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="43dp"
        tools:layout_editor_absoluteY="388dp" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rol"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="43dp"
        tools:layout_editor_absoluteY="511dp" />

    <EditText
        android:id="@+id/txtusuarioNuevo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Ingrese Usuario"
        android:inputType="text"
        tools:layout_editor_absoluteX="43dp"
        tools:layout_editor_absoluteY="304dp" />

    <EditText
        android:id="@+id/txtcontrasenaNueva"
        android:layout_width="207dp"
        android:layout_height="44dp"
        android:ems="10"
        android:hint="Ingrese Contraseña"
        android:inputType="text"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="414dp" />

    <Button
        android:id="@+id/btnAgregarUser"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Agregar"
        tools:layout_editor_absoluteX="221dp"
        tools:layout_editor_absoluteY="577dp" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="57dp"
        android:layout_height="47dp"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="27dp"
        tools:srcCompat="@tools:sample/avatars" />

</androidx.constraintlayout.widget.ConstraintLayout>