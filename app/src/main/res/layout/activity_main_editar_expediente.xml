<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/txtHistorialExp"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity_AgregarExpediente">

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Editar expediente"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="136dp"
        tools:layout_editor_absoluteY="46dp" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nombre"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="94dp" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Habitacion"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="188dp" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Categoria"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="292dp" />

    <TextView
        android:id="@+id/textView27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Diagnostico"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="390dp" />

    <TextView
        android:id="@+id/textView28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Historial"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="495dp" />

    <EditText
        android:id="@+id/txtnombreEP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Agregar nombre"
        android:inputType="text"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="116dp" />

    <EditText
        android:id="@+id/txtHabitacionEP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Agregar numero de habitacion"
        android:inputType="numberPassword"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="210dp" />

    <EditText
        android:id="@+id/txtCategoriaEP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Agregar numero de categoria"
        android:inputType="numberPassword"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="314dp" />

    <EditText
        android:id="@+id/txtdiagnosEP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Agregar Diagnostico"
        android:inputType="text"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="412dp" />

    <EditText
        android:id="@+id/txthistorialEP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Agregar Historial"
        android:inputType="text"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="517dp" />

    <Button
        android:id="@+id/btnAgregarEP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Agregar"
        tools:layout_editor_absoluteX="68dp"
        tools:layout_editor_absoluteY="600dp" />

    <Button
        android:id="@+id/btnEliminarEP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Eliminar"
        tools:layout_editor_absoluteX="255dp"
        tools:layout_editor_absoluteY="593dp" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="45dp"
        android:layout_height="44dp"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="24dp"
        tools:srcCompat="@tools:sample/avatars" />

</androidx.constraintlayout.widget.ConstraintLayout>