<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/txtHistorialExp"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity_AgregarExpediente">

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Agregar Expediente"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="136dp"
        tools:layout_editor_absoluteY="46dp" />
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.411"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.067" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="98dp"
        android:layout_height="58dp"
        android:layout_marginTop="57dp"
        android:layout_marginEnd="39dp"
        android:layout_marginBottom="520dp"
        app:layout_constraintBottom_toTopOf="@+id/btnAgregarExp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView23"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nombre"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="54dp"
        tools:layout_editor_absoluteY="111dp" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Habitacion"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="222dp" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Categoria"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="329dp" />

    <TextView
        android:id="@+id/textView27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Diagnostico"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="439dp" />

    <TextView
        android:id="@+id/textView28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Historial"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="549dp" />

    <EditText
        android:id="@+id/txtnombreEP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Agregar nombre"
        android:inputType="text"
        tools:layout_editor_absoluteX="54dp"
        tools:layout_editor_absoluteY="144dp" />

    <EditText
        android:id="@+id/txtHabitacionEP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Agregar numero de habitacion"
        android:inputType="numberPassword"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="258dp" />

    <EditText
        android:id="@+id/txtCategoriaEP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Agregar numero de categoria"
        android:inputType="numberPassword"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="361dp" />

    <EditText
        android:id="@+id/txtdiagnosEP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Agregar Diagnostico"
        android:inputType="text"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="473dp" />

    <EditText
        android:id="@+id/txthistorialEP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Agregar Historial"
        android:inputType="text"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="587dp" />

    <Button
        android:id="@+id/btnAgregarExp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Agregar"
        tools:layout_editor_absoluteX="265dp"
        tools:layout_editor_absoluteY="643dp" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="63dp"
        android:layout_height="66dp"
        tools:layout_editor_absoluteX="309dp"
        tools:layout_editor_absoluteY="57dp"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="49dp"
        android:layout_height="41dp"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="16dp"
        tools:srcCompat="@tools:sample/avatars" />


</androidx.constraintlayout.widget.ConstraintLayout>