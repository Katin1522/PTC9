<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/txtHistorialExp"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity_Horario">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Horario"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="179dp"
        tools:layout_editor_absoluteY="47dp" />

    <CalendarView
        android:id="@+id/calendarView"
        android:layout_width="292dp"
        android:layout_height="306dp"
        tools:layout_editor_absoluteX="74dp"
        tools:layout_editor_absoluteY="69dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Turnos"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="412dp" />

    <EditText
        android:id="@+id/txtAgregarturno"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Agrega un turno"
        android:inputType="text"
        tools:layout_editor_absoluteX="83dp"
        tools:layout_editor_absoluteY="565dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="90dp"
        android:layout_height="93dp"
        tools:layout_editor_absoluteX="292dp"
        tools:layout_editor_absoluteY="442dp"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dia del turno"
        tools:layout_editor_absoluteX="72dp"
        tools:layout_editor_absoluteY="458dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hora del turno"
        tools:layout_editor_absoluteX="175dp"
        tools:layout_editor_absoluteY="454dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dia del turno2"
        tools:layout_editor_absoluteX="176dp"
        tools:layout_editor_absoluteY="505dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dia del turno2"
        tools:layout_editor_absoluteX="74dp"
        tools:layout_editor_absoluteY="505dp" />

    <Button
        android:id="@+id/btnAgregarTurno"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Agregar"
        tools:layout_editor_absoluteX="258dp"
        tools:layout_editor_absoluteY="641dp" />

</androidx.constraintlayout.widget.ConstraintLayout>