<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity_Habitaciones">

    <TextView
        android:id="@+id/textView29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Habitaciones"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="158dp"
        tools:layout_editor_absoluteY="50dp" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nombre"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="64dp"
        tools:layout_editor_absoluteY="88dp" />

    <TextView
        android:id="@+id/NombreCM"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Habitacion"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="64dp"
        tools:layout_editor_absoluteY="183dp" />

    <EditText
        android:id="@+id/txtnumeroHabi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Ingrese numero de la habitacion"
        android:inputType="numberPassword"
        tools:layout_editor_absoluteX="64dp"
        tools:layout_editor_absoluteY="205dp" />

    <EditText
        android:id="@+id/txtConfirmarContra"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Ingresar el nombre"
        android:inputType="text"
        tools:layout_editor_absoluteX="64dp"
        tools:layout_editor_absoluteY="110dp" />

    <Button
        android:id="@+id/btnAgregarHabi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Agregar"
        tools:layout_editor_absoluteX="141dp"
        tools:layout_editor_absoluteY="274dp" />

    <Button
        android:id="@+id/btnHabitaciones"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Habitaciones de los pacientes"
        tools:layout_editor_absoluteX="155dp"
        tools:layout_editor_absoluteY="649dp" />

    <androidx.recyclerview.widget.RecyclerView
        android:layout_width="409dp"
        android:layout_height="289dp"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="341dp" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="37dp"
        android:layout_height="33dp"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="26dp"
        tools:srcCompat="@tools:sample/avatars" />
</androidx.constraintlayout.widget.ConstraintLayout>